{
  "manifest_version": 3,
  "name": "Simple To-Do",
  "version": "1.5",
  "description": "A to-do list extension to manage tasks by PopVia.com.",
  "permissions": [
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
